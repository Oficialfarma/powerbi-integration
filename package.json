  },
  "homepage": "https://github.com/oficialfarma-dev-projetcs/powerbi-integration#readme",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "jest": "^26.6.3",
    "sucrase": "^3.17.1",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
